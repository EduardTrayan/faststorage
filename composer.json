{
    "name": "eduardtrayan/faststorage-bundle",
    "type": "symfony-bundle",
    "description": "Bundle for encapsulation working with in-memory storage",
    "keywords": ["memcache","redis","storage"],
    "license": "MIT",
    "authors": [
        {
            "name": "Eduard Trayan",
            "email": "eduardtrayan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/framework-bundle": ">=2.3",
        "predis/predis": "~1.0",
        "ext-memcache": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": { "EduardTrayan\\FaststorageBundle": "" }
    },
    "target-dir": "EduardTrayan/FaststorageBundle"
}